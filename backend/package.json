{
  "name": "nasa-data-explorer-backend",
  "version": "1.0.0",
  "description": "NASA Data Explorer API Backend",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "postinstall": "npm run build",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "validate": "tsc --noEmit"
  },
  "keywords": ["nasa", "api", "space", "astronomy"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "express-rate-limit": "^7.1.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "@types/express": "^4.17.21",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.4",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "ts-node-dev": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
} 